<!DOCTYPE html>
<html>
<head>
  <title>TetsRails</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
<div class="container">
<% unless flash.nil? %>
	<% flash.map do |type, msg| %>
		<div class="alert alert-<%= type %>">
			<%= msg %>
		</div>
	<% end %>
<% end %>
<%= yield %>
</div>
</body>
</html>
